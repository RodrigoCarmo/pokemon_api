{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "linebreak-style": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "indent": "off",
    "no-unused-vars": "warn",
    "camelcase": "off",
    "no-shadow": "warn",
    "import/prefer-default-export": "off"
  }
}